{
    "manifest_version": 2,
    "name": "CzechEventsAddButton",
    "version": "0.0.4",
    "author": "Pavol Hejn√Ω",
    "description": "Scrape data from social networks and send it to Czech.events",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "exclude_matches": ["*://localhost:*/"],
            "css": ["./src/style.css"],
            "js": ["./src/config.js", "./src/index.js"]
        }
    ],
    "permissions": ["storage", "<all_urls>"]
}
